<!DOCTYPE xml>
<mvc:View xmlns='sap.m' xmlns:mvc="sap.ui.core.mvc" controllerName="ext.samples.taskmanagement.controller.App">
	<App>
		<Page showHeader="true" title="{i18n>appTitle}" titleAlignment="Center" class="sapUiContentPadding" floatingFooter="true">
			<VBox class="sapUiSmallMargin">
				<HBox justifyContent="End" class="sapUiSmallMarginBottom">
					<HBox id="profile" alignItems="Center">
						<Avatar displaySize="L" src="data:image/jpeg;base64,{user>/photo/base64Encoded}" />
						<Title class="sapUiTinyMarginBegin" text="{user>/defaultFullName}" />
					</HBox>
				</HBox>
				<HBox justifyContent="SpaceBetween" alignItems="Center" wrap="Wrap">
					<Title class="sapUiSmallMargin" text="{i18n>tableTitle}" titleStyle="H2" />
					<Button icon="sap-icon://add" text="{i18n>newRequestButton}" type="Emphasized" press="newRequest">
						<dependents>
							<mvc:XMLView id="selectionView" viewName="ext.samples.taskmanagement.view.UserSelection" />
						</dependents>
					</Button>
				</HBox>
				<Table id="table" items="{path:'requests>/onboardRequests', sorter:[{path:'toDoEntryV2/status'}]}">
					<dependents>
						<mvc:XMLView id="detailsView" viewName="ext.samples.taskmanagement.view.UserDetails" />
					</dependents>
					<columns>
						<Column />
						<Column hAlign="Right" />
					</columns>
					<items>
						<ColumnListItem>
							<cells>
								<VBox height="5rem">
									<Title class="sapUiSmallMarginTop" text="{requests>toDoEntryV2/toDoEntryName}" />
									<ObjectStatus class="sapUiTinyMarginTop" text="{path:'requests>toDoEntryV2/status', formatter:'.formatStatusText'}"
										state="{path:'requests>toDoEntryV2/status', formatter:'.formatStatusState'}"
										visible="{path:'requests>toDoEntryV2/status', formatter:'.formatStatusVisibility'}" 
										inverted="true" /> 
								</VBox>
								<HBox justifyContent="End">
									<Button class="sapUiTinyMarginEnd" type="Accept" icon="sap-icon://accept" press="completeRequest"
										visible="{path:'requests>toDoEntryV2/status', formatter:'.formatCompleteActionVisibility'}"
										tooltip="{i18n>completeRequestTooltip}" />
									<Button class="sapUiTinyMarginEnd" type="Ghost" icon="sap-icon://hint" press="showDetails"
										tooltip="{i18n>showEmployeeDetailsTooltip}" />
									<Button type="Reject" icon="sap-icon://decline" press="deleteRequest" 
										tooltip="{i18n>deleteRequestTooltip}"/>
								</HBox>
							</cells>
						</ColumnListItem>
					</items>
				</Table>
			</VBox>
			<footer>
				<OverflowToolbar>
					<ToolbarSpacer />
					<Image src="data:image/png;base64," />
				</OverflowToolbar>
			</footer>
		</Page>
	</App>
</mvc:View> 